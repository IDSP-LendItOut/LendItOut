<div class="onboarding-slide">
  <img class="slide-image" src="<%= slideImage %>" alt="Slide Image" />
  <h2><%= title %></h2>
  <h3><%= subtitle %></h3>
  <p><%= description %></p>

  <div class="dot-indicator">
    <span class="<%= index === 0 ? 'active' : '' %>"></span>
    <span class="<%= index === 1 ? 'active' : '' %>"></span>
    <span class="<%= index === 2 ? 'active' : '' %>"></span>
  </div>
</div>
