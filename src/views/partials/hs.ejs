<main class="listings">
    <div class="section-header">
        <h2>Some Cats</h2>
        <a href="#">See more</a>
    </div>
    <div class="scroll-container">
        <div class="item-grid">
            <% items.forEach(item => { %>
                <div class="item-card">
                    <img src="<%= item.image %>" alt="<%= item.name %>">
                    <div class="item-details">
                        <p class="item-name"><%= item.name %></p>
                        <p class="item-price"><%= item.price %></p>
                        <p class="item-status <%= item.statusClass %>"><%= item.status %></p>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>
</main>